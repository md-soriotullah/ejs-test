<%- include('layout/header') %>
    <main>
        <form action="/" method="post">
            <input type="text" placeholder="Programming Language" name="programming">
            <button type="submit">Add</button>
        </form>

        <ul>
            <% if(plNames.length > 0){ %>
               <% for(let x = 0; x< plNames.length; x++){ %>
                    <li><%= plNames %></li>
                <% } %>
            <% }else{ %>
                <p>No data found</p>
            <% } %>
        </ul>
    </main>
    <%- include('layout/footer') %>
    